#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int c = 0, b = 0;
    char s[100];
    int i;
    scanf("%s", s);
    if(s[0] == ')' || s[0] == '}' || s[strlen(s) - 1] == '{' || s[strlen(s) - 1] == '(')
        {
            printf("no1");
            return 0;
        }
    for (i = 0; s[i] != '\0'; i++)
    {
        if (s[i] == '(' )
        {
            if(s[i+1] != '}')
            {
                c++;
            }
            else
            {
                printf("no2");
                return 0;
            }
        }
        if(s[i] == '{')
            {
                    if(s[i+1] != ')')
                {
                    b++;
                }
                else
                {
                    printf("no3");
                    return 0;
                }
            }
        if(s[i] == ')' && --c < 0)
        {
            printf("no3.1");
            return 0;
        }
        if (s[i] == '}' && --b < 0)
        {
            printf("no3.2");
            return 0;
        }
    }
    if(c == 0 && b == 0)
    {
        printf("da %d %d", c, b);
        return 0;
    }
    else
    {
        printf("no4 %d %d", c, b);
        return 0;
    }
    return 0;
}
